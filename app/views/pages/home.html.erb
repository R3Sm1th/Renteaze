<div class="hero-container">
  <div class="content">
    <div>
      <h1> <strong>Your digital platform for renting property.</strong> </h1>
      <h4> Remove all the hassle in your next property move</h4>
    </div>
    <div class="btn-wrap d-flex justify-content-between align-items-center rounded-5">
    <%# <% user_is_landlord = current_user == @property.user %>
      <% if current_user && current_user.agent%>
      <%= link_to "Agent Dashboard", properties_path, class: "btn btn-lg btn-primary btn-banner m-0"%>
      <% elsif current_user && current_user.tenant%>
      <%= link_to "Tenant Dashboard",  my_applications_path, class: "btn btn-lg btn-primary btn-banner m-0" %>
      <% else%>
      <%= link_to "Sign In", user_session_path, class: "btn btn-lg btn-primary btn-banner m-0"  %>
      <% end %>
    </div>
  </div>
</div>
<%# About Section %>
  <section class="about_section">
    <div class="container">
      <div class="row">
        <div class="col-md-6 ">
          <div class="img-box">
            <img src="https://res.cloudinary.com/dcu7y5wnn/image/upload/v1688992121/prop_f0tral.jpg" alt="">
          </div>
        </div>
        <div class="col-md-6">
          <div class="detail-box">
            <div class="heading_container">
              <h2>
                ABOUT US
              </h2>
            </div>
            <p>
              Renteaze is your easy to use, free, cloud-based property management software. Key features like vetting, documentation, progress monitoring and much more on your fingertips.
            </p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- process section -->

